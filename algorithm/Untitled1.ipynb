    "def search(A):\n",
    "    n = len(A)\n",
    "    for i in  range(n-1):\n",
    "        least = i\n",
    "        for j in range(i+1,n):\n",
    "            if(A[least]>A[j]):\n",
    "                least = j\n",
    "        A[i] , A[least] = A[least] , A[i]\n",
    "        print(A, i+1,\"번째 실행결과\")"
   ]
